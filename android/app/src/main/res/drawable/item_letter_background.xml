<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape>
            <stroke
                android:width="1dp"
                android:color="@color/oldprimary" />
            <corners android:radius="6dp" />
        </shape>
    </item>

    <item android:bottom="4dp"
        android:left="4dp"
        android:right="4dp"
        android:top="4dp">
        <shape>
            <solid android:color="@color/tile_background_color"/>
            <corners android:radius="6dp"/>
        </shape>
    </item>


</layer-list>
